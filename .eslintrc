{
  "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6
        
   },
  "extends": "yandex",
  "env": {
    "browser": true
  },
  "rules": {
  
        "indent": ["error", 4],
        "linebreak-style": ["error", "windows"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],   
        "comma-dangle": ["error", "always"],
        "no-cond-assign": ["error", "always"],
        "no-console": "off",
  }


  
}
